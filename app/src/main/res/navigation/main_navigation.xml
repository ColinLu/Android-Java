<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.colin.android.demo.java.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.colin.android.demo.java.ui.method.MethodFragment"
        android:label="@string/method"
        tools:layout="@layout/fragment_method" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.colin.android.demo.java.ui.view.ViewFragment"
        android:label="@string/view"
        tools:layout="@layout/layout_list" />
</navigation>