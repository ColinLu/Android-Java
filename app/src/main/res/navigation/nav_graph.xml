<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/GoogleMapFragment">
    <navigation
        android:id="@+id/navigation"
        app:startDestination="@id/GoogleMapFragment">
        <fragment
            android:id="@+id/MethodFragment"
            android:name="com.colin.android.demo.java.ui.method.MethodFragment"
            android:label="@string/method"
            tools:layout="@layout/layout_list">
            <action
                android:id="@+id/action_Method_to_Log"
                app:destination="@+id/LogFragment"
                app:enterAnim="@animator/nav_default_enter_anim"
                app:exitAnim="@animator/nav_default_exit_anim"
                app:popEnterAnim="@animator/nav_default_pop_enter_anim"
                app:popExitAnim="@animator/nav_default_pop_exit_anim" />
            <action
                android:id="@+id/action_Method_to_ContactList"
                app:destination="@+id/ContactListFragment" />
            <action
                android:id="@+id/action_Method_to_Path"
                app:destination="@+id/PathFragment" />
            <action
                android:id="@+id/action_Method_to_Http"
                app:destination="@+id/HttpFragment" />
        </fragment>
    </navigation>

    <fragment
        android:id="@+id/HomeFragment"
        android:name="com.colin.android.demo.java.ui.home.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_Home_to_View"
            app:destination="@+id/ViewFragment" />
        <action
            android:id="@+id/action_Home_to_Method"
            app:destination="@id/navigation" />
        <action
            android:id="@+id/action_Home_to_WebIndex"
            app:destination="@+id/WebIndexFragment" />
    </fragment>

    <fragment
        android:id="@+id/ViewFragment"
        android:name="com.colin.android.demo.java.ui.view.ViewFragment"
        android:label="@string/view">
        <action
            android:id="@+id/action_View_to_Motion"
            app:destination="@+id/MotionFragment" />
        <action
            android:id="@+id/action_View_to_Text"
            app:destination="@+id/TextFragment" />
        <action
            android:id="@+id/action_View_to_Circle"
            app:destination="@+id/CircleImageFragment" />
        <action
            android:id="@+id/action_View_to_behavior"
            app:destination="@+id/BehaviorFragment" />
        <action
            android:id="@+id/action_View_to_banner"
            app:destination="@+id/BannerFragment" />
    </fragment>
    <fragment
        android:id="@+id/WebIndexFragment"
        android:name="com.colin.android.demo.java.ui.web.WebIndexFragment"
        android:label="@string/web"
        tools:layout="@layout/fragment_web_index">
        <action
            android:id="@+id/action_webIndex_to_web"
            app:destination="@+id/WebViewFragment">
            <argument
                android:name="type"
                android:defaultValue="0"
                app:argType="integer" />
            <argument
                android:name="url"
                android:defaultValue="www.baidu.com"
                app:argType="string" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/LogFragment"
        android:name="com.colin.android.demo.java.ui.method.LogFragment"
        android:label="@string/log"
        tools:layout="@layout/fragment_log" />
    <fragment
        android:id="@+id/HttpFragment"
        android:name="com.colin.android.demo.java.ui.method.HttpFragment"
        android:label="@string/http"
        tools:layout="@layout/fragment_http" />
    <fragment
        android:id="@+id/PathFragment"
        android:name="com.colin.android.demo.java.ui.method.PathFragment"
        android:label="@string/path"
        tools:layout="@layout/layout_list" />
    <fragment
        android:id="@+id/MotionFragment"
        android:name="com.colin.android.demo.java.ui.view.MotionFragment"
        android:label="@string/motion"
        tools:layout="@layout/fragment_motion" />

    <fragment
        android:id="@+id/ContactListFragment"
        android:name="com.colin.android.demo.java.ui.method.ContactListFragment"
        android:label="@string/contact"
        tools:layout="@layout/layout_list" />

    <fragment
        android:id="@+id/TextFragment"
        android:name="com.colin.android.demo.java.ui.view.TextFragment"
        android:label="@string/text"
        tools:layout="@layout/fragment_text" />

    <fragment
        android:id="@+id/CircleImageFragment"
        android:name="com.colin.android.demo.java.ui.view.CircleImageFragment"
        android:label="@string/circle"
        tools:layout="@layout/fragment_circle_image" />
    <fragment
        android:id="@+id/BehaviorFragment"
        android:name="com.colin.android.demo.java.ui.view.BehaviorFragment"
        android:label="@string/behavior"
        tools:layout="@layout/fragment_behavior" />
    <fragment
        android:id="@+id/WebViewFragment"
        android:name="com.colin.android.demo.java.ui.web.WebViewFragment"
        android:label="@string/web"
        tools:layout="@layout/fragment_web_view">

        <argument
            android:name="type"
            android:defaultValue="0"
            app:argType="integer" />
        <argument
            android:name="url"
            android:defaultValue="www.baidu.com"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/BannerFragment"
        android:name="com.colin.android.demo.java.ui.view.BannerFragment"
        android:label="@string/banner"
        tools:layout="@layout/fragment_banner" />

    <fragment
        android:id="@+id/AMapFragment"
        android:name="com.colin.library.android.map.amap.AMapFragment"
        android:label="@string/amap_map3d_app_name"
        tools:layout="@layout/fragment_a_map" />

    <fragment
        android:id="@+id/GoogleMapFragment"
        android:name="com.colin.library.android.map.google.GoogleMapFragment"
        android:label="@string/amap_map3d_app_name"
        tools:layout="@layout/fragment_google_map" />
</navigation>